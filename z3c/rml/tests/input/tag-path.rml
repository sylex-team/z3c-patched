      <moveto>
        8cm 19cm
      </moveto>
      6cm 19cm
      <moveto>
        4cm 19cm
      </moveto>
      3cm 20.5cm
    </path>

    <path x="2cm" y="15cm" fill="true"
          doc:example="">
      8cm 15cm
      <curvesto>
        10cm 15cm 10cm 12cm 8cm 12cm
      </curvesto>
      2cm 12cm
      <moveto doc:example="">
        2cm 15cm
      </moveto>
    </path>

    <path x="8cm" y="12cm" fill="true"
          doc:example="">
      <curveto doc:example="">
        10cm 12cm 10cm 9cm 8cm 9cm
      </curveto>
    </path>

  </pageDrawing>
</document>
