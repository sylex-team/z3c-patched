<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE document SYSTEM "rml.dtd">

<document
    filename="tag-registerType1Face.pdf"
    xmlns:doc="http://namespaces.zope.org/rml/doc">
